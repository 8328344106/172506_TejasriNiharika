<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
	<bean id="AddressBean" class="com.Address.Address">
		<property name="street" value="Shabul Bazar"/>
		<property name="city" value="challapalli" />
		<property name="state" value="Andrapradesh"/>
		<property name="zip" value="521126" />
		<property name="country" value="India"/>
	</bean>
	<bean id="CustomerBean" class="com.Customer.Customer">
		<property name="customerId" value="172506"/>
		<property name="customerName" value="Niharika"/>
		<property name="customerContact" value="9133822089"/>
		<property name="customerAddress" ref="AddressBean" />
	</bean>
</beans> 